{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "rules": {
    "quotes": [1, "single"],
    "semi": [2, "never"],
    "comma-dangle": [2, "always-multiline"],
    "no-console": [0]
  },
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": "6",
    "sourceType": "module"
  }
}
