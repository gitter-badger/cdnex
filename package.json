{
  "name": "cdnex",
  "version": "0.1.0",
  "description": "prepends cdn urls to your html and css",
  "author": "Jason Maurer <j@maur.co>",
  "license": "MIT",
  "homepage": "https://github.com/jsonmaur/cdnex",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsonmaur/cdnex"
  },
  "keywords": [
    "cdn",
    "cdnify",
    "prepend",
    "url",
    "rewrite",
    "regex",
    "production",
    "build",
    "html",
    "css"
  ],
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib && babel src -d lib --source-maps true --no-comments",
    "lint": "eslint src",
    "istanbul": "npm run build && istanbul cover tests/index.test.js | tap-spec",
    "coveralls": "cat coverage/lcov.info | node_modules/coveralls/bin/coveralls.js && rm -rf coverage",
    "test": "npm run lint && npm run istanbul && npm run coveralls",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "babel-cli": "6.5.1",
    "babel-preset-es2015": "6.5.0",
    "coveralls": "2.11.6",
    "eslint": "2.2.0",
    "istanbul": "0.4.2",
    "tap-spec": "4.1.1",
    "tape": "4.4.0"
  },
  "dependencies": {
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
