{
  "name": "cdnex",
  "version": "0.1.0",
  "description": "prepend cdn urls to your source files",
  "author": "Jason Maurer <j@maur.co>",
  "license": "MIT",
  "homepage": "https://github.com/jsonmaur/cdnex",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsonmaur/cdnex"
  },
  "keywords": [
    "cdn",
    "cdnify",
    "prepend",
    "url",
    "rewrite",
    "regex",
    "production",
    "build",
    "html",
    "css",
    "cli"
  ],
  "main": "lib",
  "bin": {
    "cdnex": "bin/cdnex"
  },
  "scripts": {
    "lint": "eslint src",
    "pretest": "npm run lint",
    "test": "istanbul cover tape tests/cdnex.test.js | tap-spec",
    "prebuild": "rm -rf lib",
    "build": "babel src -d lib --source-maps true --no-comments",
    "watch": "npm run build -- -w",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "babel-cli": "6.5.1",
    "babel-eslint": "5.0.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-stage-3": "6.5.0",
    "coveralls": "2.11.6",
    "eslint": "2.2.0",
    "istanbul": "0.4.2",
    "tap-spec": "4.1.1",
    "tape": "4.4.0"
  },
  "dependencies": {
    "babel-polyfill": "6.5.0",
    "chalk": "1.1.1",
    "commander": "2.9.0",
    "denodeify": "1.2.1",
    "fs-extra": "0.26.5",
    "fs-promise": "0.4.1",
    "glob": "7.0.0",
    "inquirer": "0.12.0",
    "update-notifier": "0.6.0",
    "validator": "4.9.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-3"
    ]
  }
}
